{
  "name": "backend",
  "version": "1.3.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node -r ts-node/register src/index.ts",
    "dev": "node --inspect --watch --watch-path=./src -r ts-node/register src/index.ts",
    "test": "vitest run",
    "migration:generate": "npx drizzle-kit generate",
    "migration:migrate": "npx drizzle-kit migrate",
    "migration:seed": "node -r ts-node/register ./src/db/seed.ts",
    "db:seed:generate": "node -r ts-node/register src/db/generate_seed.ts",
    "db:seed:dump": "node -r ts-node/register src/db/generate_seed.ts",
    "db:seed:apply": "node -r ts-node/register src/db/seed.ts"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.21.0",
    "axios": "^1.7.7",
    "body-parser": "^2.0.1",
    "chokidar": "^3.6.0",
    "config": "^3.3.12",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.33.0",
    "esbuild-register": "^3.6.0",
    "express": "^4.20.0",
    "express-session": "^1.18.0",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "kafka-node": "^5.0.0",
    "lodash": "^4.17.21",
    "openai": "^4.76.3",
    "passport": "^0.7.0",
    "passport-gitlab2": "^5.0.0",
    "postgres": "^3.4.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@testcontainers/kafka": "^10.24.2",
    "@testcontainers/postgresql": "^10.24.2",
    "@types/node": "^22.4.1",
    "drizzle-kit": "^0.24.2",
    "supertest": "^7.1.0",
    "testcontainers": "^10.24.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.5.4",
    "vitest": "^3.1.2"
  }
}
